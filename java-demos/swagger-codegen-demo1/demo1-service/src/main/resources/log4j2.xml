<Configuration monitorInterval="30" verbose="true">

  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
      <!--<ConnectJsonLayout/>-->
    </Console>
  </Appenders>

  <Loggers>
    <AsyncRoot level="trace">
      <AppenderRef ref="Console"/>
    </AsyncRoot>
  </Loggers>

</Configuration>
